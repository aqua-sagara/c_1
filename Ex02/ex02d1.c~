#include <stdio.h>
main()
{
  int 0,1,2,3,4,x;
  printf("enter x\n");
  scanf("%d",&x);
  printf("x:y\n");
  0=((x-2)^2-4*(x-2)+3);
  1=((x-1)^2-4*(x-1)+3);
  2=(x^2-4*x+3);
  3=((x+1)^2-4*(x+1)+3);
  4=((x+2)^2-4*(x+2)+3);
  printf("0:%d\n",0);
  printf("1:%d\n",1);
  printf("2:%d\n",2);
  printf("3:%d\n",3);
  printf("4:%d\n",4);
}
