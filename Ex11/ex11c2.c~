#include <stdio.h>
main()

int InMat[3][4],OutMat[6][2],i,j,m,n;
  printf("３行４列の行列を整数値で入力して下さい。 \n");
for(i=0;i<3;i++)
  {
    for(j=0;j<4;j++)
      {
	scanf("%d",&InMat[i][j]);
      }
  }
printf("６行２列に変換した行列を表示します。\n");
for(i=0;i<3;i++)
  {
    for(j=0;j<4;j++)
      {
	for(m=0;m<3;m++)
	  {
	    for(n=0;n<4;n++)
	      {
		OutMat[m][n]=InMat[i][j];
		printf("%d ",OutMat[m][n]);

	      }
	    printf("\n");
	  }
      }
  }
}

 
