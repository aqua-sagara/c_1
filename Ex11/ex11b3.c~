#include <stdio.h>
main()
{
  int a[10][3],n,i,b,c;
  for(i=0;i<10;i++){
    a[i][0]=i+1;
  }
  for(i=0;i<10;i++){
    a[i][2]=a[i][1]*a[i][1];
  }
  for(i=0;i<10;i++){
    a[i][3]=a[i][1]*a[i][1]*a[i][1];
  }
  printf("３乗値を入力してください！\n");
  scanf("%d",n);
  for(i=0;i<10;i++){
  if(a[i][3]=n)
    b=a[i][1];
    c=a[i][2];
  prinf("%dの３乗根=%d\n",n,b);
  printf("%dの2乗=%d\n",n,c);
  }
}
