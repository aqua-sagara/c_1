int voidd2b(int n, int c){
    int q;
    int t;
    int s[8];
    int i;
    for(i=7;i>=0;i--)
      {
	q=n%2;
	t=n/2;
	n=t;
	s[i]=q;
      
      }
    return s[c];
  }
    
